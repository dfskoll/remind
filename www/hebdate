echo Content-type: text/html
echo ""

cat <<EOM
<HTML>
<HEAD>
<TITLE>Hebrew date</TITLE>
</HEAD>
<BODY>

<H1>Hebrew date</H1>

EOM

$REMIND -arqh $DIR/hebdate.rem

echo "</BODY>"
echo "</HTML>"
exit 0
